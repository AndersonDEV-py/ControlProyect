	<queryString>
		<![CDATA[select   COALESCE(cc.nombre1,'')||' '|| COALESCE(cc.nombre2,'')||' '||COALESCE(cc.apellido1,'')||' '||COALESCE(cc.apellido2,'') as nombre,
         COALESCE(cc.apellido1, '') apellido1,
         COALESCE(cc.apellido2, '') apellido2,
         COALESCE(cc.nombre1,'')||' '|| COALESCE(cc.nombre2,'')  as nombres,
         (select
  			COALESCE(mu.nombre,'')||', '|| COALESCE(d.nombre,'') as lugarn
		  from  u_afinsol.ad_solicitudes as s
		  inner join u_afinsol.ad_conozca_cliente as cc
      		on cc.liteuid = s.liteuidcasc
 		  inner join u_afinsol.ad_departamentos as d
	  		on cc.departamentonac = cast(d.id as varchar)
		  inner join u_afinsol.ad_municipios as mu
	  		on cc.municipionac = cast(mu.id as varchar)
		  inner join u_afinsol.ad_paises as pa
			on d.idpais =  cast(pa.id as varchar)
		  where s.liteuid = $P{liteuid}
		  and cc.departamentonac = cast(mu.iddepartamento as varchar)
		  and cc.departamentonac = cast(d.id as varchar)
		  and cc.municipionac = cast(mu.id as varchar))as lugarn,
         cc.fechanacimiento,
		 (current_date - cc.fechanacimiento)/365 as edad,
         COALESCE(pa.nacionalidad,'')  nacionalidad,
         COALESCE(cc.identidad,'') identidad,
         COALESCE(cc.tipodocumento,'') tipodocumento,
         COALESCE(cc.generos,'')  sexo,
         COALESCE(cc.estadocivil, '') estadocivil,
         COALESCE(cc.conyuguenombre1,'')||' '||COALESCE(cc.conyuguenombre2,'')||' '||COALESCE(cc.conyugueapellido1,'')||' '||COALESCE(cc.conyugueapellido2,'') as conyuge,
         (COALESCE(b.nombre,'') ||COALESCE((' Ave'||' '||  cc.direccionavenida),'')||COALESCE((', Calle'||' '||  cc.direccioncalle),'')||COALESCE(( ' Bloque'||' '||  cc.direccionbloque),'')
	     ||COALESCE((' Casa No.'||' '||  cc.direccioncasano),'')||COALESCE((', Sector'||' '||  cc.direccionsector ||', '),'')||COALESCE(mu.nombre,'')||', '|| COALESCE(d.nombre,'')) as direccion,
		 COALESCE(cc.direcciontelefonofijo, 'N/A') as telresidencia,
         COALESCE(cc.direccioncorreo, 'N/A') direccioncorreo,
		 COALESCE(pg.valor,'') as profesionofisio,
       (select
			COALESCE(b.nombre,'')||' '||COALESCE((' Ave'||' '||  cc.negavenida),'')||COALESCE((', Calle'||' '||  cc.negcalle),'')||COALESCE(( ' Bloque'||' '||  cc.negbloque),'')||
			COALESCE((', Sector'||' '||  cc.negsector||', '),'')||COALESCE((' Casa No.'||' '||  cc.negnumerocasa),'')
			||COALESCE(' '||mu.nombre,'')||', '|| COALESCE(d.nombre,'') as direccionneg
		 from  u_afinsol.ad_solicitudes as s
		 inner join u_afinsol.ad_conozca_cliente as cc
     		 on cc.liteuid = s.liteuidcasc
		 inner join
	 		 u_afinsol.ad_barrios as b
	 		 on cc.negbarrio = cast(b.id as varchar)
		 inner join
	  		 u_afinsol.ad_departamentos as d
	  		 on cc.negdepartamento = cast(d.id as varchar)
		inner join
	 		u_afinsol.ad_municipios as mu
	  		on cc.negmunicipio = cast(mu.id as varchar)
		inner join u_afinsol.ad_paises as pa
			on d.idpais =  cast(pa.id as varchar)
		where s.liteuid = $P{liteuid}
		and cc.negdepartamento = cast(mu.iddepartamento as varchar)
		and cc.negdepartamento = cast(d.id as varchar)
		and cc.negmunicipio = cast(mu.id as varchar)
		and cc.negbarrio = cast(b.id as varchar)
		and cc.negmunicipio = cast(b.idmunicipio as varchar)
		and cc.negdepartamento = cast(b.iddepartamento as varchar)
		and cc.negsector = cast(b.idsector as varchar)) as direccionneg,
		 COALESCE(cc.negtelefononegocio, 'N/A') negtelefononegocio,
         COALESCE(sv.poliza, 'N/A')  poliza,
         COALESCE(sv.detallepoliza, 'N/A')  detallepoliza,
         COALESCE(s.liteuidcasc,'') liteuidcasc,
         COALESCE(s.montosolicitado, 0) montosolicitado,
         COALESCE(sv.peso,0) peso,
         COALESCE(sv.altura,0)  altura,
         COALESCE(sv.cancer, '')  cancer,
         COALESCE(sv.presion,'')  presion,
         COALESCE(sv.epilepsia, '')  epilepsia,
         COALESCE(sv.diabetes, '')  diabetes,
         COALESCE(sv.rinones, '')  rinones,
         COALESCE(sv.tumores, '')  tumores,
         COALESCE(sv.corazon, '')  corazon,
         COALESCE(sv.mentales, '')  mentales,
         COALESCE(sv.otras, '')  otras,
         COALESCE(sv.embarazo, '') embarazo,
         COALESCE(sv.meses, 0) meses,
         sv.fechap,
         s.fecharegistro,
		 COALESCE(ag.agencia, '')  agencia,
		 COALESCE( cast((SELECT EXTRACT(DAY FROM s.fecharegistro)) as integer) ,0) dia,
         COALESCE((select REPLACE(
           	REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(
           	REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(
           	REPLACE(to_char(s.fecharegistro, 'Month'),
           						'January','Enero'),
           						'February','Febrero'),
           						'March','Marzo'),
           						'April','Abril'),
           					    'May','Mayo'),
           						'June','Junio'),
           						'July','Julio'),
           						'August','Agosto'),
           						'September','Septiembre'),
           						'October','Octubre'),
           						'November','Noviembre'),
           						'December','Diciembre')) ,'N/A') mes,
         COALESCE(cast((SELECT EXTRACT(YEAR FROM s.fecharegistro)) as integer ) ,0) anio,
	     COALESCE((select nombre from u_afinsol.ad_usuarios where usuariotopaz=cc.usuariotopaz),' ') nombreasesor
from u_afinsol.ad_seguro_vida sv inner join u_afinsol.ad_solicitudes as s
          on sv.liteuidsol = s.liteuid
          inner join u_afinsol.ad_conozca_cliente as cc
          on cc.liteuid = s.liteuidcasc
		  inner join agenda_digital.ad_agencias as ag
		   on cast(ag.agenciaid as varchar) =s.agenciaid
		  inner join u_afinsol.ad_paramgral as pg
		  	on cc.profesionofisio = pg.clave
		  inner join
	  u_afinsol.ad_barrios as b
	  	on cc.direccionbarrio = cast(b.id as varchar)
	  inner join
	  u_afinsol.ad_departamentos as d
	  	on cc.direcciondepartamento = cast(d.id as varchar)
	 inner join
	 u_afinsol.ad_municipios as mu
	  	on cc.direccionmunicipio = cast(mu.id as varchar)
	inner join u_afinsol.ad_paises as pa
		on d.idpais =  cast(pa.id as varchar)
where s.liteuid = $P{liteuid}
and cc.liteuid = s.liteuidcasc
and pg.grupo = 'PROFESIONES'
and cc.direcciondepartamento = cast(mu.iddepartamento as varchar)
and cc.direcciondepartamento = cast(d.id as varchar)
and cc.direccionmunicipio = cast(mu.id as varchar)
and cc.direccionbarrio = cast(b.id as varchar)
and cc.direccionmunicipio = cast(b.idmunicipio as varchar)
and cc.direcciondepartamento = cast(b.iddepartamento as varchar)
and cc.direccionsector = cast(b.idsector as varchar)]]>
	</queryString>
