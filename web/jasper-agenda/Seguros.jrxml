	<subDataset name="enfermedades" whenResourceMissingType="Empty" uuid="03bb4d52-c88e-4cb0-9fa4-490fbb236d45">
		<parameter name="liteuid_enfermedades" class="java.lang.String" isForPrompting="false"/>
		<queryString>
			<![CDATA[select *
from (
          select 'Cáncer'  tipo, cancerdetalle  detalle from u_afinsol.ad_seguro_vida where cancer = 'S' and liteuidsol = $P{liteuid_enfermedades}
          union all
          select 'Diabetes'  tipo, diabetesdetalle  detalle from u_afinsol.ad_seguro_vida where diabetes= 'S' and liteuidsol = $P{liteuid_enfermedades}
          union all
          select 'Corazón'  tipo, corazondetalle  detalle from u_afinsol.ad_seguro_vida where corazon= 'S' and liteuidsol = $P{liteuid_enfermedades}
          union all
          select 'Presión Arterial Alta'  tipo, presiondetalle  detalle from u_afinsol.ad_seguro_vida where presion= 'S'  and liteuidsol = $P{liteuid_enfermedades}
          union all
          select 'Enfermedad de Riñones'  tipo, rinonesdetalle  detalle from u_afinsol.ad_seguro_vida where rinones= 'S'  and liteuidsol = $P{liteuid_enfermedades}
          union all
          select 'Enfermedad Mentales'  tipo, mentalesdetalle  detalle from u_afinsol.ad_seguro_vida where mentales= 'S'  and liteuidsol = $P{liteuid_enfermedades}
          union all
          select 'Epilepsia'  tipo, epilepsiadetalle  detalle from u_afinsol.ad_seguro_vida where epilepsia= 'S'  and liteuidsol = $P{liteuid_enfermedades}
          union all
          select 'Tumores'  tipo, tumoresdetalle  detalle from u_afinsol.ad_seguro_vida where tumores= 'S'  and liteuidsol = $P{liteuid_enfermedades}
          union all
          select 'Otras'  tipo, otrasdetalle  detalle from u_afinsol.ad_seguro_vida where otras= 'S'  and liteuidsol = $P{liteuid_enfermedades}
   )subq limit 3]]>
		</queryString>
		<field name="tipo" class="java.lang.String"/>
		<field name="detalle" class="java.lang.String"/>
	</subDataset>
